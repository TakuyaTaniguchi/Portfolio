-
	var ogp = {
		'title':'Rin Works!!!',
		'description':'TakuyaTaniguchi WebDeveloper Portfolio',
		'url':'test',
		'image':'http://portfolio.littledemon.pw/images/ogp.png',
		'siteName':'Rin Works!!!',
		'lang':'ja',
		'fbid':'test',
		'fbid':'test'
	}
-
	var meta = {
		title:'Rin Works!!!',
		desc:'TakuyaTaniguchi WebDeveloper Portfolio',
	}
include _inc_mixins
include _inc_conditional
block slug
	- var slug = ''
	- var is_contact = false;
head
	block meta_data
		meta(http-equiv='X-UA-Compatible', content='IE=edge')
		meta(charset='UTF-8')
		meta#viewport(content='width=device-width', name='viewport')
		<meta name="twitter:card" content="summary_large_image">
		<meta name="twitter:title" content="Rin Works!!!">
		<meta name="twitter:description" content="TakuyaTaniguchi WebDeveloper Portfolio">
		<link href="https://fonts.googleapis.com/css?family=Vibur&display=swap" rel="stylesheet">
		<link href="https://fonts.googleapis.com/css?family=Nixie+One&display=swap" rel="stylesheet">
		<link rel="icon" href="https://portfolio.littledemon.pw/images/c_r.ico">
		+base_meta(meta)
		+ogp(ogp)
	block css
		include _inc_css
body#top(class=slug)
	#wrap
		block header
			.l-header
				.l-header_inner
					h1.l-header_title.u-neon
						a.l-header_title_link(href="/") Works!!
					nav.p-nav
						.p-nav_inner
							ul.p-nav_list
								li.p-nav_list_item
									a.p-nav_list_item_link.u-neonLink.-is-green(href="/") Home
								li.p-nav_list_item
									a.p-nav_list_item_link.u-neonLink.-is-green(href="./private.html") BusinessWorks
								li.p-nav_list_item
									a.p-nav_list_item_link.u-neonLink.-is-blue(href="https://github.com/TakuyaTaniguchi" target="_blank") Github
		block contents
		block flg
			#responsive_flg
		//- #totop
		//- 	a(href="#top") トップ
	block scripts
		include _inc_scripts

</html>
